local function icon(id, x)
    local b = Instance.new("ImageButton", Header)
    b.Position = UDim2.new(0, x, 0, 8)
    b.Size = UDim2.new(0, 22, 0, 22)
    b.BackgroundTransparency = 1
    b.Image = "rbxassetid://" .. id
    b.ImageColor3 = Color3.fromRGB(120, 120, 120)
    return b
end

local CONSOLE_ICON = icon("7743872929", 476)
local EXEC_ICON = icon("11570895459", 508)
